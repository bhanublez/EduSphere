        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@android:drawable/ic_delete"
        android:label="@string/app_name"
        android:roundIcon="@android:drawable/ic_delete"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dynamic_view"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".studentLayouts.dashBoard" />
        <activity android:name=".teacherLayouts.addAssignment" />
        <activity android:name=".teacherLayouts.addExam"/>

        <activity android:name=".studentLayouts.TeacherView"/>
        <activity android:name=".studentLayouts.subjectView"/>
        <activity android:name=".studentLayouts.examView"/>
        <activity android:name=".studentLayouts.assignmentView"/>
        <activity android:name=".studentLayouts.profileView"/>



    </application>

</manifest>