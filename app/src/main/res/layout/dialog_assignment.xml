<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="match_parent"

     android:layout_width="match_parent"
    tools:ignore="MissingDefaultResource">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cardAssignment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/curve_bg_ek1"
        android:padding="15dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="1dp">


        <TextView
            android:id="@+id/last_submission_date_3_ek1"
            android:layout_width="115dp"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:text="Marks Alloted"
            android:textColor="@color/assign_date_3_color"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="@+id/last_submission_date_3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/last_submission_date_3"
            app:layout_constraintTop_toBottomOf="@+id/last_submission_date_3_ek1T"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/nameSub_va"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:hint="Subject Name Here"
            android:textColor="@color/structure_of_atoms_color"
            android:textSize="18dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/teacherNaam_va"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/teacherNaam_va" />

        <TextView
            android:id="@+id/assign_date_3"
            android:layout_width="115dp"
            android:layout_height="32dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp"
            android:text="Assigned Date"

            android:textColor="@color/assign_date_3_color"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="@+id/nameSub_va"
            app:layout_constraintTop_toBottomOf="@+id/nameSub_va" />

        <TextView
            android:id="@+id/last_submission_date_3"
            android:layout_width="115dp"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:text="Submission Date"
            android:textColor="@color/assign_date_3_color"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="@+id/assign_date_3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/assign_date_3"
            app:layout_constraintTop_toBottomOf="@+id/assign_date_3" />

        <TextView
            android:id="@+id/last_submission_date_3_ek1T"
            android:layout_width="115dp"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:text="Set"
            android:textColor="@color/assign_date_3_color"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="@+id/last_submission_date_3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/last_submission_date_3"
            app:layout_constraintTop_toBottomOf="@+id/last_submission_date_3"
            tools:ignore="DuplicateIds" />

        <TextView
            android:id="@+id/assDate_va"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginStart="20dp"
            android:textColor="@color/__10_oct_20_color"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/assign_date_3"
            app:layout_constraintEnd_toStartOf="@+id/guideline2"
            app:layout_constraintStart_toEndOf="@+id/assign_date_3"
            app:layout_constraintTop_toTopOf="@+id/assign_date_3"
            app:layout_constraintVertical_bias="1.0" />

        <TextView
            android:id="@+id/setId_va"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:textColor="@color/__10_oct_20_color"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/marks_va"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/marks_va"
            app:layout_constraintTop_toBottomOf="@+id/marks_va" />

        <TextView
            android:id="@+id/marks_va"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:textColor="@color/__10_oct_20_color"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/subDate_va"
            app:layout_constraintStart_toStartOf="@+id/subDate_va"
            app:layout_constraintTop_toBottomOf="@+id/subDate_va" />

        <TextView
            android:id="@+id/subDate_va"
            android:layout_width="wrap_content"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:textColor="@color/__10_oct_20_color"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/assDate_va"
            app:layout_constraintStart_toStartOf="@+id/assDate_va"
            app:layout_constraintTop_toBottomOf="@+id/assDate_va" />

        <TextView
            android:id="@+id/teacherNaam_va"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:hint="Teacher Name Here"
            android:textColor="@color/science_color"
            android:textSize="18dp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/delID"
            android:layout_width="32dp"
            android:layout_height="34dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/teacherNaam_va"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/teacherNaam_va"
            app:srcCompat="@drawable/delete_icon" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.0" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.9232614" />

        <EditText
            android:id="@+id/data_c"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:layout_marginTop="8dp"
            android:gravity="start|top"
            android:textColor="@color/__10_oct_20_color"
            android:textSize="15dp"
            android:textStyle="bold"
            android:inputType="textMultiLine"
            app:layout_constraintEnd_toEndOf="@+id/textView5"
            app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:text="Data"
            android:textColor="@color/__10_oct_20_color"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/last_submission_date_3_ek1"
            app:layout_constraintStart_toStartOf="@+id/last_submission_date_3_ek1"
            app:layout_constraintTop_toBottomOf="@+id/last_submission_date_3_ek1" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>